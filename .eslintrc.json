{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": [2,"always"],

    "camelcase": 2,

    "eqeqeq":2,

    "no-restricted-globals":0,

    "indent": 2,

    "quotmark": "single",

    "no-console" : 0,

    "prefer-destructuring" : 0,

    "no-nested-ternary" : 0,

    "no-unused-expressions" : 0
  }
}
